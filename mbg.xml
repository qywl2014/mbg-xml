<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="postgresql-9.4.1212.jar" />

  <context id="pg" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="org.postgresql.Driver"
        connectionURL="jdbc:postgresql://localhost:5432/postgres"
        userId="postgres"
        password="wulang123">
    </jdbcConnection>

    <javaModelGenerator targetPackage="com.wulang.model" targetProject="src/main/java">
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="test.xml"  targetProject="src/main/resources">
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.wulang.dao"  targetProject="src/main/java">
    </javaClientGenerator>

    <table schema="public" tableName="country" domainObjectName="Customer" >
      <generatedKey column="id" sqlStatement="MySql" />
    </table>

  </context>
</generatorConfiguration>